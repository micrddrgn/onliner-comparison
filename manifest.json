{
  "manifest_version": 2,

  "name": "Onliner - сравнение товаров",
  "description": "Сравнение товаров сайта catalog.onliner.by со страниц их технического описания",
  "version": "0.0.1",
  "icons": {
    "16": "icons/16.png",
    "48": "icons/48.png",
    "128": "icons/128.png"
  },
  "browser_action": {
    "default_icon": "icons/48.png",
    "default_popup": "popup.html"
  },
  "permissions": [
    "storage",
    "tabs",
    "http://catalog.onliner.by/*"
  ]
}
